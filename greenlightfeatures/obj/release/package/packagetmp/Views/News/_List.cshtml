@model IEnumerable<ELearning.Models.News>

<div class="list-group">
    @foreach (var item in Model)
    {
        <h5><i class="fa fa-calendar"></i> @Html.DisplayFor(modelItem => item.Title)</h5>
        <p class="text-muted">@Html.DisplayFor(modelItem => item.PubDate)</p>
        <p>@Html.DisplayFor(modelItem => item.Details)</p>

    }
</div>