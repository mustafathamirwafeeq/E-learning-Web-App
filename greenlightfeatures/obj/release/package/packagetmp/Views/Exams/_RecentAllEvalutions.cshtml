@model IEnumerable<ELearning.Models.Evaluation>
<table class="table">
    <tr>
        <th>Exam</th>        
        <th>Cours</th>
        <th>Name</th>
        <th>Grade</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @item.Exam.Title
            </td>
            <td>
                @item.Exam.Cours.Title
            </td>
            <td>
                @ELearning.Models.Utilities.GetUserFullName(item.StudentId)
            </td>
            <td>
                @item.Grade
            </td>
        </tr>
    }
</table>
